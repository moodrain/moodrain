<?php
require_once('vendor/autoload.php');
$optionFormat = 'f';
try
{
    $config = new Muyu\Config();
} catch (Exception $e)
{
    echo $e->getMessage() . PHP_EOL;
    $config = new Muyu\Config([]);
}
$command = new Muyu\Command(
    $config('command.host', 'https://moodrain.cn'),
    $config('command.user', null),
    $config('command.pass', null)
);
$option = getopt($optionFormat);
$command->read($argv, $option);